fun fib(u64:u64) {
    let n: u64;
    if (n 2u64 less(u64,u64:u8))
        n
    else 
        n 2u64 sub(u64,u64:u64)
        n 1u64 sub(u64,u64:u64)
        fib(u64:u64)
        swap(u64,u64:u64,u64)
        fib(u64:u64)
        add(u64,u64:u64);
}
