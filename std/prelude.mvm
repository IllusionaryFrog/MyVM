
import "asm.mvm";
import "string.mvm";

fun{unsafe} __start(string:) {
    let args: string;

    4000u64
    dup(u64:u64,u64)
    alloc(u64:u64)
    add(u64,u64:u64)
    pop_sp(u64:)

    1000u64
    dup(u64:u64,u64)
    alloc(u64:u64)
    add(u64,u64:u64)
    pop_cs(u64:)

    "args: '" write(string:u64) drop(u64:)
    args write(string:u64) drop(u64:)
    "'\n\n" write(string:u64) drop(u64:)

    main(:)
}
